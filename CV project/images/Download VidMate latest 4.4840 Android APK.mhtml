------MultipartBoundary--JErmmUof7hn23x8dv1Yr3MexllDH1EX1b3nRZRg1K3----
Content-Type: text/css
Content-Transfer-Encoding: binary
Content-Location: cid:css-3424fb94-539d-47f3-926d-d5d78570f727@mhtml.blink

@charset "utf-8";

.box-block-title { padding: 10px 15px; line-height: 30px; overflow: hidden; color: rgb(85, 85, 85); font-size: 1.4rem; background: rgb(255, 255, 255); }

.box-block-title .tit { font-size: 1.4rem; color: rgb(85, 85, 85); text-overflow: ellipsis; overflow: hidden; white-space: nowrap; margin-right: 75px; }

.box-block-title .more { float: right; }

.box-block-title .more a { color: rgb(102, 102, 102); font-size: 1.4rem; }

.box-block-title .arrow-right { display: inline-block; position: relative; top: -1px; width: 6px; height: 6px; border-width: 2px 0px 0px 2px; border-top-style: solid; border-left-style: solid; border-top-color: rgb(134, 134, 134); border-left-color: rgb(134, 134, 134); border-image: initial; border-right-style: initial; border-right-color: initial; border-bottom-style: initial; border-bottom-color: initial; transform: rotate(135deg); }

.ar_fix .box-block-title .tit { margin-right: 0px; margin-left: 75px; }

.ar_fix .box-block-title .more { float: left; }

.ar_fix .box-block-title .arrow-right { transform: rotate(315deg); }

.fast-download-box { text-align: center; background: rgb(255, 255, 255); overflow: hidden; padding-bottom: 10px; border-bottom: 1px solid rgb(224, 224, 224); }

.fast-download-box .tit { background: rgb(255, 255, 255); border-radius: 8px; margin: 10px; border: 1px solid rgb(36, 205, 119); }

.fast-download-box .tit .bd, .fast-download-box .tit .fast { color: rgb(255, 255, 255); background-color: rgb(36, 205, 119); border-bottom-left-radius: 8px; border-bottom-right-radius: 8px; }

.fast-download-box .tit .fast.on { border-bottom-left-radius: unset; border-bottom-right-radius: unset; }

.fast-download-box .tit .fast i { border-top: 4px solid rgb(255, 255, 255); border-left: 4px solid transparent; border-right: 4px solid transparent; display: inline-block; position: relative; top: -2px; }

.fast-download-box .tit .fast i.hover { border-bottom: 4px solid rgb(255, 255, 255); border-top: none; }

.fast-download-box h1 { font-weight: normal; font-size: 1.6rem; line-height: 2.6rem; color: rgb(69, 121, 221); padding: 10px 0px; }

.fast-download-box .file { color: rgb(51, 51, 51); display: block; font-size: 1.4rem; overflow-wrap: break-word; word-break: normal; line-height: 2.2rem; }

.fast-download-box .fsize { color: rgb(119, 119, 119); }

.fast-download-box .fast-des { padding: 8px 15px; text-align: initial; line-height: 2.4rem; }

.fast-download-box p { line-height: 2.6rem; font-size: 1.2rem; color: rgb(119, 119, 119); }

.fast-download-box a { color: rgb(69, 121, 221); }

.fast-download-box .tit .bd a { color: rgb(255, 255, 255); }

.fast-download-box .tit .bd a .how-a { text-decoration: underline; }

.fast-introduction { display: none; background: white; }

.fast-download-share-btn { margin-top: 10px; background: rgb(255, 255, 255); padding: 10px 8px; border-top: 1px solid rgb(224, 224, 224); height: 32px; text-align: center; white-space: nowrap; text-overflow: ellipsis; }

.fast-download-box .tit .bd { line-height: 2.6rem; font-size: 1.4rem; }

.fast-introduction ul li { display: flex; background: rgb(246, 247, 249); margin: 0px 8px 8px; border: 1px solid rgb(224, 224, 224); border-radius: 4px; padding: 15px 20px; }

.step-des { overflow-wrap: break-word; word-break: break-all; flex: 1 1 0%; padding-left: 15px; text-align: initial; line-height: 20px; overflow: hidden; text-overflow: ellipsis; display: -webkit-box; -webkit-line-clamp: 2; -webkit-box-orient: vertical; }

.step-1, .step-2, .step-3, .step-4 { display: flex; flex-direction: column; vertical-align: middle; width: 40px; height: 40px; background: url("https://static.apkpure.com/mobile/static/imgs/fast_step.png") -9999px -9999px / 40px no-repeat; }

.step-1 { background-position: 0px 0px; }

.step-2 { background-position: 0px -40px; }

.step-3 { background-position: 0px -80px; }

.step-4 { background-position: 0px -120px; }

.slide-wrap { direction: ltr; background: white; position: relative; overflow: hidden; margin: 0px auto 10px; border-top: 1px solid rgb(224, 224, 224); border-bottom: 1px solid rgb(224, 224, 224); }

.slide-wrap .hd { height: 20px; line-height: 20px; position: absolute; bottom: 0px; right: 0px; z-index: 1; }

.slide-wrap .hd li { font-size: 0px; display: inline-block; width: 8px; height: 8px; border-radius: 8px; background: rgb(153, 153, 153); text-indent: -9999px; overflow: hidden; margin: 0px 6px; }

.slide-wrap .hd li.on { background: rgb(36, 205, 119); }

.slide-wrap .bg { height: 90px; }

.slide-wrap .bd { position: absolute; z-index: 0; width: 100%; height: 100%; }

.slide-wrap .bd li a { -webkit-tap-highlight-color: rgba(0, 0, 0, 0); display: block; position: relative; height: 70px; padding: 10px; color: rgb(85, 85, 85) !important; }

.slide-wrap .info { height: 70px; overflow: hidden; display: flex; flex-direction: column; justify-content: space-between; }

.slide-wrap .icon { position: absolute; top: 10px; left: 10px; }

.slide-wrap .info .text { font-size: 1.2rem; overflow: hidden; height: 40px; line-height: 20px; }

.slide-wrap .info .text p { word-break: break-all; overflow-wrap: break-word; }

.slide-wrap .pad-icon { padding-left: 80px; }

.slide-wrap .pad-banner { padding-left: 153px; }

.slide-wrap .info .btn { display: inline-block; background: white; color: rgb(36, 205, 119); border: 1px solid rgb(36, 205, 119); padding: 3px 10px; font-size: 1.3rem; text-align: center; border-radius: 4px; }

#iframe_download { display: none; }

.aegon-down-box { padding: 10px; background-color: white; border-top: 1px solid rgb(224, 224, 224); border-bottom: 1px solid rgb(224, 224, 224); margin: 10px 0px; }

.aegon-down-box > a { display: block; position: relative; height: 80px; color: rgb(85, 85, 85) !important; }

.aegon-down-box .icon { position: absolute; }

.aegon-down-box .icon img { width: 162px; height: 79px; }

.aegon-down-box .text { padding-left: 172px; position: relative; height: 80px; }

.aegon-down-box .text .tit { font-size: 14px; font-weight: 400; line-height: 18px; color: rgb(33, 33, 33); }

.aegon-down-box .text .des { color: rgb(117, 117, 117); font-size: 12px; font-weight: 300; overflow: hidden; text-overflow: ellipsis; display: -webkit-box; -webkit-line-clamp: 2; -webkit-box-orient: vertical; }

.aegon-down-box .text .btn { position: absolute; bottom: 0px; padding: 0px 20px; line-height: 23px; font-size: 12px; background: rgb(255, 255, 255); text-align: center; color: rgb(36, 205, 119); text-decoration: none; overflow: hidden; border: 1px solid rgb(36, 205, 119); }

.ar_fix .aegon-down-box .text { padding-left: inherit; padding-right: 172px; }

.ar_fix .step-des { padding-left: 0px; padding-right: 15px; }
